{
  "name": "fusion.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cli": "cd cli && npm start",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "start": "cd ./server && npm run-script start",
    "install_": "npm install && cd ./client && npm install && cd ./../server && npm install"
  },
  "dependencies": {
    "concurrently": "^4.0.1"
  }
}
