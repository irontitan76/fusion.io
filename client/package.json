{
  "name": "fusion.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@fortawesome/fontawesome-pro": "^5.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.5",
    "@fortawesome/free-brands-svg-icons": "^5.4.0",
    "@fortawesome/pro-light-svg-icons": "^5.4.0",
    "@fortawesome/pro-regular-svg-icons": "^5.4.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@svgr/webpack": "2.4.1",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.3",
    "bcryptjs": "^2.4.3",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "fusioncharts": "^3.13.3-sr.1",
    "graphql": "^0.13.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.includes": "^4.3.0",
    "lodash.omit": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.values": "^4.3.0",
    "mini-css-extract-plugin": "0.4.3",
    "moment": "^2.23.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.4",
    "react-dom": "^16.5.2",
    "react-fusioncharts": "^3.0.0",
    "react-intl": "^2.7.2",
    "react-markdown": "^4.0.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "validator": "^10.8.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "engines": {
    "node": ">=10.4.1",
    "npm": ">=6.1.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "deploy": "gcloud app deploy",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "proxy": "http://localhost:8081/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "array-bracket-spacing": "error",
      "brace-style": "error",
      "camelcase": ["error", {
        "allow": [
          "^.*(campaign|code|domain|email|feature|identity|locale|product|session|subscription|time|url).*$"
        ]
      }],
      "capitalized-comments": "error",
      "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline"
      }],
      "comma-spacing": "error",
      "comma-style": "error",
      "func-call-spacing": "error",
      "import/no-unresolved": "off",
      "indent": ["warn", 2],
      "jsx-a11y/label-has-for": "off",
      "jsx-a11y/label-has-associated-control": [2, {
        "controlComponents": [
          "Button",
          "Input"
        ]}
      ],
      "jsx-a11y/media-has-caption": "off",
      "jsx-quotes": ["warn", "prefer-single"],
      "keyword-spacing": "error",
      "lines-between-class-members": "error",
      "max-depth": ["error", 3],
      "max-len": ["error", 100],
      "multiline-comment-style": ["error", "starred-block"],
      "no-array-constructor": "error",
      "no-await-in-loop": "error",
      "no-extra-parens": "error",
      "no-lonely-if": "error",
      "no-unused-vars": "error",
      "no-console": "warn",
      "no-debugger": "error",
      "no-else-return": "error",
      "no-multiple-empty-lines": "error",
      "no-nested-ternary": "error",
      "no-trailing-spaces": "error",
      "no-unneeded-ternary": "error",
      "no-unresolved": "off",
      "no-useless-computed-key": "error",
      "no-useless-constructor": "error",
      "no-useless-escape": "error",
      "no-useless-rename": "error",
      "no-var": "error",
      "no-whitespace-before-property": "error",
      "object-curly-spacing": "error",
      "padded-blocks": ["error", "never"],
      "prefer-const": "error",
      "prefer-destructuring": "error",
      "prefer-object-spread": "error",
      "prefer-spread": "error",
      "react/jsx-filename-extension": "off",
      "react/jsx-no-duplicate-props": ["error", {
        "ignoreCase": false
      }],
      "react/jsx-sort-props": ["error", {
        "ignoreCase": true,
        "reservedFirst": false
      }],
      "semi": "error",
      "semi-style": ["error", "last"],
      "spaced-comment": "error",
      "space-before-blocks": "error",
      "space-infix-ops": "error",
      "space-in-parens": "error",
      "sort-keys": ["error", "asc", {
        "caseSensitive": false
      }],
      "sort-vars": "error"
    },
    "globals": {
      "Promise": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "plugins": [
        "react"
      ]
    }
  }
}
