{
  "name": "fusion.io--server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rm -rf build/* && babel -d ./build ./server.js && babel -d ./build/api api && babel -d ./build/config config && babel -d ./build/graphql graphql && babel -d ./build/mongoose mongoose",
    "deploy": "gcloud app deploy",
    "dev": "babel-node server.js",
    "start": "NODE_ENV=production node ./build/server.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "mongoose": "^5.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
